<div class='float-right'>
  <%= link_to 'New', new_dashboard_week_path, class: 'btn btn-primary' %>
</div>

<h1>Weeks</h1>
<hr/>
<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>User</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @weeks.each do |week| %>
      <tr>
        <td><%= week.name %></td>
        <td><%= week.user_id %></td>
        <td><%= link_to 'Show', week %></td>
        <td><%= link_to 'Edit', edit_week_path(week) %></td>
        <td><%= link_to 'Destroy', week, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>


<%= render 'shared/paginate', collection: @weeks %>
